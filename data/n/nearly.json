[{"word":"nearly","type":"adverb","pronunc":{"uk":{"mp3":"http://dictionary.cambridge.org/media/english/uk_pron/u/ukn/uknav/uknaval023.mp3","ogg":"http://dictionary.cambridge.org/media/english/uk_pron_ogg/u/ukn/uknav/uknaval023.ogg","text":"ˈnɪə.li"},"us":{"mp3":"http://dictionary.cambridge.org/media/english/us_pron/n/nea/nearl/nearly.mp3","ogg":"http://dictionary.cambridge.org/media/english/us_pron_ogg/n/nea/nearl/nearly.ogg","text":"ˈnɪr.li"}},"definitions":[{"definition":"almost, or not completely: ","examples":["It's been nearly three months since my last haircut."]},{"definition":"a lot less: ","examples":["She's not nearly as beautiful as you said she was."]},{"definition":"much less than you want or need: ","examples":["There's not nearly enough food for all these people!"]}]}]