[{"word":"unusually","type":"adverb","pronunc":{"uk":{"mp3":"http://dictionary.cambridge.org/media/english/uk_pron/u/uku/ukunt/ukuntim016.mp3","ogg":"http://dictionary.cambridge.org/media/english/uk_pron_ogg/u/uku/ukunt/ukuntim016.ogg","text":"ʌnˈjuː.ʒu.ə.li"},"us":{"mp3":"http://dictionary.cambridge.org/media/english/us_pron/u/unu/unusu/unusually.mp3","ogg":"http://dictionary.cambridge.org/media/english/us_pron_ogg/u/unu/unusu/unusually.ogg","text":"ʌnˈjuː.ʒu.ə.li"}},"definitions":[{"definition":"more than is usual or expected, or in a way that is not usual: ","examples":["He was unusually polite."]},{"definition":"in a way that is not usual for someone: ","examples":["Unusually for me, I felt deeply embarrassed."]}]}]