[{"word":"reasonably","type":"adverb","pronunc":{"uk":{"mp3":"http://dictionary.cambridge.org/media/english/uk_pron/u/ukr/ukrea/ukreapp012.mp3","ogg":"http://dictionary.cambridge.org/media/english/uk_pron_ogg/u/ukr/ukrea/ukreapp012.ogg","text":"ˈriː.zə"},"us":{"mp3":"http://dictionary.cambridge.org/media/english/us_pron/r/rea/reaso/reasonably.mp3","ogg":"http://dictionary.cambridge.org/media/english/us_pron_ogg/r/rea/reaso/reasonably.ogg","text":"ˈriː.zə"}},"definitions":[{"definition":"using good judgment: ","examples":["Stop shouting and let's discuss this reasonably."]},{"definition":"in a satisfactory way: ","examples":["She writes reasonably good children's books."]},{"definition":"at a price that is not too expensive: ","examples":["You can eat out very reasonably these days."]}]}]