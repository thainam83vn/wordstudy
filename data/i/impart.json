[{"word":"impart","type":"verb","pronunc":{"uk":{"mp3":"http://dictionary.cambridge.org/media/english/uk_pron/u/uki/ukimp/ukimpar001.mp3","ogg":"http://dictionary.cambridge.org/media/english/uk_pron_ogg/u/uki/ukimp/ukimpar001.ogg","text":"ɪmˈpɑːt"},"us":{"mp3":"http://dictionary.cambridge.org/media/english/us_pron/i/imp/impar/impart.mp3","ogg":"http://dictionary.cambridge.org/media/english/us_pron_ogg/i/imp/impar/impart.ogg","text":"ɪmˈpɑːrt"}},"definitions":[{"definition":"to communicate information to someone: ","examples":["to impart the bad news"]},{"definition":"to give something a particular feeling, quality, or taste: ","examples":["Preservatives can impart colour and flavour to a product."]}]}]